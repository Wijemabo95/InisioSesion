<script>

    var inicioDeSesionRegistrado = "Wijemabo95";
    var contrasenhaRegistrada = "william95";
    var intentos = 3;
    var contador = 1;
    var inicioDeSesionIngresado;
    var contrasenhaIngresada;
    


    while(contador <= intentos) { 

        inicioDeSesionIngresado = prompt("Ingrese su usuario");

        if (inicioDeSesionIngresado === inicioDeSesionRegistrado) {
            contrasenhaIngresada = prompt("Ingrese su contraseña");   
            if (contrasenhaIngresada === contrasenhaRegistrada) {
                alert("Bienvenido " + inicioDeSesionRegistrado);
                break;
            } else {
                alert("Contraseña erronea");
            } 
        } else{
        alert("Usuario erroneo");
        }
        contador++;
    }
</script>
